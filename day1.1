#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(void) {
  FILE *readFile;
  readFile = fopen("input.txt", "r");
  char segment[1000];
  char tmp[1000];
  int i;
  int sum = 0;

  while (fgets(segment, sizeof(segment), readFile) != NULL) {

    for (i = 0; i < strlen(segment); i++) {
        if (strchr("0123456789", segment[i]) != NULL) {
            tmp[0] = segment[i];
            tmp[1] = '\0';
            break;
        }
    }

    for (i = strlen(segment) - 1; i >= 0; i--) {
        if (strchr("0123456789", segment[i]) != NULL) {
            tmp[1] = segment[i];
            tmp[2] = '\0';
            break;
        }
    }
    
    sum += atoi(tmp);
    
    for (i = 0; i < strlen(tmp); i++) {
      tmp[i] = '\0';
    }
  }

  printf("%d\n", sum);
}
